{
    "name": "php-task/task-bundle",
    "description": "Symfony integration for php-task library",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Wachter",
            "email": "johannes.wachter@massiveart.at"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "php-task/php-task": "dev-master as 2.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/expression-language": "^5.4 || ^6.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/process": "^5.4 || ^6.0",
        "doctrine/orm": "^2.5.3"
    },
    "require-dev": {
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "symfony/finder": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^5.4 || ^6.0",
        "doctrine/doctrine-bundle": "^1.5 || ^2.0",
        "doctrine/data-fixtures": "^1.3.3"
    },
    "autoload": {
        "psr-4": {
            "Task\\TaskBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Task\\TaskBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
